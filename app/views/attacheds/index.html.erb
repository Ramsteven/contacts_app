<div>
  <div>
    <div class="container text-center">
      <h1>List attached files</h1>

            <td><%=link_to "Delete all", destroy_all_path(current_user), method: :delete, class: "btn-sm btn-info" %></td>
      <div class="flickr_pagination m-4">
        <%= will_paginate @attacheds, :container => false %>
      </div>
      <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">File</th>
            <th scope="col">Status</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
          <% @attacheds.each do | attch |%>
          <tr>
            <th scope="row"><%= attch.id %></th>
            <td><%= attch.name %></td>
            <td><%= attch.status %></td>
            <td><%=link_to "import", attacheds_import_path(attch), method: :post, class: "btn-sm btn-info" %></td>
          </tr>
          <%end%>
        </tbody>
      </table>
      <div class="flickr_pagination m-4">
        <%= will_paginate @attacheds, :container => false %>
      </div>

    </div>
  </div>
</div>
